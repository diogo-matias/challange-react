{
    "expo": {
        "name": "Loja de Brinquedos",
        "slug": "loja-brinquedos",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "light",
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "NSAppTransportSecurity": {
                    "NSAllowsArbitraryLoads": true
                }
            }
        },
        "android": {
            "usesCleartextTraffic": true
        },
        "web": {
            "bundler": "webpack",
            "webpackConfig": "./webpack.config.js"
        }
    }
}